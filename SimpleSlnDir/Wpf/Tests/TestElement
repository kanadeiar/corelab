namespace Wpf.Tests;

public class TestElement : UIElement
{
    public static readonly DependencyProperty SampleProperty = DependencyProperty
        .Register("Sample", typeof(int), typeof(TestElement),
        new FrameworkPropertyMetadata(default(int)));
    public int Sample
    {
        get => (int)GetValue(SampleProperty);
        set => SetValue(SampleProperty, value);
    }
    static TestElement()
    {

    }
}
