@model IEnumerable<string>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Фичи языка</title>
</head>

<body>
    <ul>
        @foreach (var s in Model)
        {
            <li>@s</li>
        }
    </ul>
</body>

</html>