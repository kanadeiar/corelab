namespace Wpf.Tests;

public class TestElement : UIElement
{
    public static readonly DependencyProperty SampleProperty;
    public int Sample
    {
        get => (int)GetValue(SampleProperty);
        set => SetValue(SampleProperty, value);
    }
    static TestElement()
    {
        var metadata = new FrameworkPropertyMetadata(default(int));
        SampleProperty = DependencyProperty.Register("Sample", typeof(int), typeof(TestElement), metadata);
    }
}
