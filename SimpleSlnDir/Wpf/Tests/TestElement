namespace Wpf.Tests;

public class TestElement : UIElement
{
    public static readonly DependencyProperty SampleProperty;
    public Thickness Sample
    {
        get => (Thickness)GetValue(SampleProperty);
        set => SetValue(SampleProperty, value);
    }
    static TestElement()
    {
        var metadata = new FrameworkPropertyMetadata(new Thickness());
        SampleProperty = DependencyProperty.Register("Sample", typeof(Thickness), typeof(FrameworkElement), metadata);
    }
}
